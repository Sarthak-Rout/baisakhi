        crossorigin="anonymous">
 <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
 <div class="row">
     <div class="col-md-2 admin-dashboard">
         <%- include('tables') %>
     </div>
     <div class="col-md-10">
        <div class = "container">
            <div class="wrapper">
                <form action="/eventMaster" method="POST" >
                    <h3 class="form-signin-heading">EVENT ENTRY</h3>
                <hr class="colorgraph"><br>
                <label><b>Event Name:</b></label>
                <input type="text" class="form-control" name="nameEvent" placeholder="Event Name" required="" autofocus="" />
                <label><b>Location:</b></label>
                <input type="text" class="form-control" name="location" placeholder="Location" required="" autofocus="" />
                <label><b>From:</b></label>
                <input type="date" class="form-control" name="dateFrom" placeholder="" required="" autofocus="" />
                <label><b>To:</b></label>
                <input type="date" class="form-control" name="dateFrom" placeholder="" required="" autofocus="" />
                <label><b>No of Days:</b></label>
                <input type="number" class="form-control" name="district" placeholder="no of days" required="" autofocus="" />
                <button class="btn btn-lg btn-primary btn-block" type="Submit">Add</button>
                </form>
            </div>
        </div>
     </div>
 </div>
