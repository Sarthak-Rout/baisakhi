<h3>Stall Allocation</h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Sl No</th>
            <th>Vendor Name</th>
            <th>Stall Name</th>
            <th>Stall Type</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
       <% for(var i = 0; i < stall.length; i++){ %>
        <tr>
            <td><%= i+1 %></td>
            <td><%= stall[i].name %></td>
            <td><%= stall[i].stallName %></td>
            <td><%= stall[i].stallType %></td>
        </tr>
    <% } %>
    </tbody>
</table>